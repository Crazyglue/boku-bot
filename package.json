{
  "name": "boku-bot",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "zip -r function.zip . -x '*.git*'",
    "deploy": "rm function.zip && npm run build && npm run push:s3 && npm run update:lambda",
    "update:lambda": "/usr/local/bin/aws2 lambda update-function-code --function-name BokuLambda --region us-west-2 --s3-bucket boku-bot-builds --s3-key function.zip",
    "push": "/usr/local/bin/aws2 lambda update-function-code --function-name BokuLambda --zip-file fileb://function.zip --region us-west-2",
    "push:s3": "/usr/local/bin/aws2 s3 cp function.zip s3://boku-bot-builds",
    "test": "jest"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "axios": "^0.19.2",
    "dotenv": "^8.2.0",
    "https": "^1.0.0",
    "querystring": "^0.2.0"
  },
  "devDependencies": {
    "aws-sdk": "^2.610.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-plugin-import": "^2.20.0",
    "eslint-plugin-jest": "^23.6.0",
    "jest": "^25.1.0"
  }
}
